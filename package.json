{
  "name": "tech-read",
  "description": "Collection tech articles daily from UGC communities",
  "author": "pomy",
  "version": "1.1.0",
  "scripts": {
    "dev-build": "webpack --config ./config/webpack.config.js -w --progress --display-error-details",
    "pro-build": "webpack --config ./config/webpack.config.js --progress --display-error-details",
    "start": "NODE_ENV=development node --harmony ./server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwqs/tech-read.git"
  },
  "license": "MIT",
  "keywords": [
    "tech-read",
    "programmer",
    "UGC",
    "programming"
  ],
  "devDependencies": {
    "babel": "5.8.29",
    "babel-core": "5.8.33",
    "babel-loader": "5.3.3",
    "babel-eslint": "4.1.8",
    "webpack": "1.12.2",
    "css-loader": "0.21.0",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "style-loader": "0.13.0",
    "eslint": "1.8.0",
    "eslint-loader": "1.1.0"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "flux": "^2.1.1",
    "co-views": "^0.2.0",
    "koa": "^0.20.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.8",
    "koa-logger": "^1.2.2",
    "koa-router": "^5.0.0",
    "koa-static": "^1.4.5",
    "swig": "^1.3.2",
    "request": "^2.65.0",
    "cheerio": "^0.20.0",
    "koa-json": "^1.1.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": 0,
      "no-cond-assign": 0,
      "no-extra-boolean-cast": 0,
      "no-unused-vars": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended"
  },
  "maintainers": [
    {
      "name": "pomy",
      "email": "pomysky@gmail.com"
    }
  ]
}