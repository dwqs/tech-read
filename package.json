{
  "name": "web-fe-dev",
  "description": "Simple front-end development scaffolding helper",
  "author": "pomy",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack --progress --display-error-details",
    "dev": "webpack-dev-server --quiet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwqs/web-fe-dev.git"
  },
  "license": "MIT",
  "keywords": [
    "web-fe-dev",
    "webpack",
    "react",
    "scaffolding"
  ],
  "devDependencies": {
    "babel": "5.8.29",
    "babel-core": "5.8.33",
    "babel-loader": "5.3.3",
    "babel-eslint": "4.1.8",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1",
    "css-loader": "0.21.0",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "style-loader": "0.13.0",
    "eslint": "1.8.0",
    "eslint-loader": "1.1.0"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": 0,
      "no-cond-assign": 0,
      "no-extra-boolean-cast": 0,
      "no-unused-vars": 0
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  },
  "maintainers": [
    {
      "name": "pomy",
      "email": "pomysky@gmail.com"
    }
  ]
}